{
	"name": "davidsoderberg/Codeblock",
	"description": "The Codeblock application.",
	"keywords": ["codeblock", "methods", "functions", "blocks"],
	"license": "MIT",
	"type": "project",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/davidsoderberg/iseed"
      }
    ],
	"require": {
		"laravel/framework": "5.1.4",
        "illuminate/html": "@dev",
        "davidsoderberg/iseed": "master",
        "firebase/php-jwt": "1.*",
        "laravel/socialite": "2.*",
        "yzalis/identicon": "*",
        "textalk/websocket": "1.0.*",
        "cboden/ratchet": "0.3.*",
        "venturecraft/revisionable": "1.*",
		"erusev/parsedown": "1.5.0",
		"erusev/parsedown-extra": "0.7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.4.0@stable",
        "fzaninotto/faker": "~1.4"
	},
	"autoload": {
		"classmap": [
			"database"
		],
        "psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
          "tests/TestTrait.php",
          "tests/UnitCase.php",
          "tests/IntegrationCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
